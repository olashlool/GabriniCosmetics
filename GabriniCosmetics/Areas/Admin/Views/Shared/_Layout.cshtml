<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="keywords" content="">

    <link rel="shortcut icon" href="~/images/gabrinicosmetics/gabrini-icone.png" type="image/x-icon">

    <title>Gabrini Cosmetics - Dashboard</title>

    <!-- Google font-->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Tajawal:wght@200;300;400;500;700;800;900&display=swap" rel="stylesheet">

    <!--Font awesome-->
    <link href="~/admin/font-awesome-pro-6.2.1-main/css/all.min.css" rel="stylesheet" />

    <!-- Bootstrap css-->
    <link rel="stylesheet" type="text/css" href="~/admin/css/vendors/bootstrap.css">

     <!-- Plugins css start-->
    <link rel="stylesheet" type="text/css" href="~/admin/css/vendors/scrollbar.css">
    <link rel="stylesheet" type="text/css" href="~/admin/css/vendors/animate.css">
    <link rel="stylesheet" type="text/css" href="~/admin/css/vendors/datatables.css">
    <link href="~/admin/css/vendors/owlcarousel.css" rel="stylesheet" />
    <!-- App css-->
    <link rel="stylesheet" type="text/css" href="~/admin/css/style.css">
    <!-- Responsive css-->
    <link rel="stylesheet" type="text/css" href="~/admin/css/responsive.css">
    <link href="~/admin/css/tagify.css" rel="stylesheet" />
    <link href="~/admin/css/intltelinput.min.css" rel="stylesheet" />
</head>
<body>
    <!-- page-wrapper Start-->
    <div class="page-wrapper compact-wrapper" id="pageWrapper">
        <div class="page-header row">
            <div class="header-logo-wrapper col-auto">
                <div class="logo-wrapper" style="max-width: 200px;">
                    <a href="/Admin">
                        <img class="img-fluid for-light" src="~/images/gabrinicosmetics/gabrini-logo-text.png" alt="" />
                        <img class="img-fluid for-dark" src="~/images/gabrinicosmetics/gabrini-logo-text.png" alt="" />
                    </a>
                </div>
            </div>
            <div class="col-4 col-xl-4 page-title">
                <h4 class="f-w-700">Admin Management</h4>
            </div>
            <!-- Page Header Start-->
            <div class="header-wrapper col m-0">
                <div class="row">
                    <div class="header-logo-wrapper col-auto p-0">
                        <div class="logo-wrapper" style="max-width: 200px;">
                            <a href="/Admin">
                                <img class="img-fluid" src="~/images/gabrinicosmetics/gabrini-logo-text.png" alt="">
                            </a>
                        </div>
                        <div class="toggle-sidebar">
                            <i class="stroke-icon sidebar-toggle status_toggle middle fa-light fa-bars-staggered" style="color:white; font-size:16px;"></i>
                            @* <svg class="stroke-icon sidebar-toggle status_toggle middle">
                                <use href="https://admin.pixelstrap.net/mofi/assets/svg/icon-sprite.svg#toggle-icon"></use>
                            </svg> *@
                        </div>
                    </div>
                    <div class="nav-right col-xxl-8 col-xl-6 col-md-7 col-8 pull-right right-header p-0 ms-auto">
                        <ul class="nav-menus">
                            <li class="profile-nav onhover-dropdown px-0 py-0">
                                <div class="d-flex profile-media align-items-center">
                                    <img class="img-30" src="~/admin/images/dashboard/profile.png" alt="">
                                    <div class="flex-grow-1">
                                        <span>@User.Identity.Name!</span>
                                        <p class="mb-0 font-outfit">Administration Manager<i class="fa fa-angle-down"></i></p>
                                    </div>
                                </div>
                                <ul class="profile-dropdown onhover-show-div">
                                    <li><a href="~/Account/logout"><i data-feather="log-in"> </i><span>Log out</span></a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <!-- Page Header Ends-->
        </div>
        <!-- Page Body Start-->
        <div class="page-body-wrapper">
            <!-- Page Sidebar Start-->
            <div class="sidebar-wrapper" data-layout="stroke-svg">
                <div>
                    <div class="logo-wrapper" style="max-width: 200px;">
                        <a href="/Admin"><img class="img-fluid" src="~/images/gabrinicosmetics/gabrini-logo-text.png" alt=""></a>
                        <div class="back-btn"></div>
                        <div class="toggle-sidebar">
                            <i class="stroke-icon sidebar-toggle status_toggle middle fa-light fa-bars-staggered" style="color:white; font-size:16px;"></i>
                            <i class="fill-icon sidebar-toggle status_toggle middle fa-light fa-bars-staggered" style="color:white; font-size:16px;"></i>
                        </div>
                    </div>
                    <div class="logo-icon-wrapper" style="max-width: 75px;padding: 12px;">
                        <a href="/Admin"><img class="img-fluid" src="~/images/gabrinicosmetics/gabrini-icone.png" alt=""></a>
                    </div>
                    <nav class="sidebar-main">
                        <div class="left-arrow" id="left-arrow"><i data-feather="arrow-left"></i></div>
                        <div id="sidebar-menu">
                            <ul class="sidebar-links" id="simple-bar">
                                <li class="back-btn">
                                    <a href="/Admin"><img class="img-fluid" src="~/images/gabrinicosmetics/gabrini-icone.png" alt=""></a>
                                    <div class="mobile-back text-end"><span>Back</span><i class="fa fa-angle-right ps-2" aria-hidden="true"></i></div>
                                </li>
                                <li class="sidebar-main-title">
                                    <div>
                                        <h6 class="lan-1">General</h6>
                                    </div>
                                </li>
                                <li class="sidebar-list">
                                    <a class="sidebar-link sidebar-title link-nav" href="/Admin">

                                        <i class="stroke-icon fa-cash-register fa-thin fa-house"></i>
                                        <i class="fill-icon fa-cash-register fa-thin fa-house"></i>

                                        <span>Dashboard</span><div class="according-menu"><i class="fa fa-angle-right"></i></div>
                                    </a>
                                </li>
                                <li class="sidebar-main-title">
                                    <div>
                                        <h6>Ecommerce</h6>
                                    </div>
                                </li>
                                <li class="sidebar-list">
                                    <a class="sidebar-link sidebar-title link-nav" href="/admin/Categories">

                                        <i class="stroke-icon fa-light fa-album-collection"></i>
                                        <i class="fill-icon fa-light fa-album-collection"></i>

                                        <span>Categories</span><div class="according-menu"><i class="fa fa-angle-right"></i></div>
                                    </a>
                                </li>
                                <li class="sidebar-list">
                                    <a class="sidebar-link sidebar-title link-nav" href="/admin/SubCategories">

                                        <i class="stroke-icon fa-light fa-layer-group"></i>
                                        <i class="fill-icon fa-light fa-layer-group"></i>

                                        <span>Sub Categories</span><div class="according-menu"><i class="fa fa-angle-right"></i></div>
                                    </a>
                                </li>
                                <li class="sidebar-list">
                                    <a class="sidebar-link sidebar-title link-nav" href="/admin/Products">

                                        <i class="stroke-icon fa-light fa-cart-shopping"></i>
                                        <i class="fill-icon fa-light fa-cart-shopping"></i>

                                        <span>Products</span><div class="according-menu"><i class="fa fa-angle-right"></i></div>
                                    </a>
                                </li>
                                <li class="sidebar-list">
                                    <a class="sidebar-link sidebar-title link-nav" href="/admin/DealOfTheDays">

                                        <i class="stroke-icon fa-light fa-cart-shopping"></i>
                                        <i class="fill-icon fa-light fa-cart-shopping"></i>

                                        <span>Deal Of The Days</span><div class="according-menu"><i class="fa fa-angle-right"></i></div>
                                    </a>
                                </li>
                                <li class="sidebar-list">
                                    <a class="sidebar-link sidebar-title link-nav" href="/admin/order">

                                        <i class="fa-regular fa-bags-shopping"></i>
                                        <i class="fill-icon fa-regular fa-bags-shopping"></i>

                                        <span>Order</span><div class="according-menu"><i class="fa fa-angle-right"></i></div>
                                    </a>
                                </li>
                                <li class="sidebar-main-title">
                                    <div>
                                        <h6 class="lan-8">Applications</h6>
                                    </div>
                                </li>
                                <li class="sidebar-list">
                                    <a class="sidebar-link sidebar-title link-nav" href="/admin/AnnouncementBar">

                                        <i class="stroke-icon fa-sharp fa-regular fa-images"></i>
                                        <i class="fill-icon fa-sharp fa-regular fa-images"></i>

                                        <span>Announcement Bar</span><div class="according-menu"><i class="fa fa-angle-right"></i></div>
                                    </a>
                                </li>
                                <li class="sidebar-list">
                                    <a class="sidebar-link sidebar-title link-nav" href="/admin/SliderBanner">

                                        <i class="stroke-icon fa-sharp fa-regular fa-images"></i>
                                        <i class="fill-icon fa-sharp fa-regular fa-images"></i>

                                        <span>Slider banner</span><div class="according-menu"><i class="fa fa-angle-right"></i></div>
                                    </a>
                                </li>
                                <li class="sidebar-list">
                                    <a class="sidebar-link sidebar-title link-nav" href="/admin/SliderAd">

                                        <i class="stroke-icon fa-sharp fa-light fa-images"></i>
                                        <i class="fill-icon fa-sharp fa-light fa-images"></i>

                                        <span>Slider Ad</span><div class="according-menu"><i class="fa fa-angle-right"></i></div>
                                    </a>
                                </li>
                                <li class="sidebar-list">
                                    <a class="sidebar-link sidebar-title link-nav" href="/admin/Feedback">

                                        <i class="stroke-icon fa-sharp fa-light fa-comments"></i>
                                        <i class="fill-icon fa-sharp fa-light fa-comments"></i>

                                        <span>Feedback</span><div class="according-menu"><i class="fa fa-angle-right"></i></div>
                                    </a>
                                </li>
                                <li class="sidebar-list">
                                    <a class="sidebar-link sidebar-title link-nav" href="/admin/discount">

                                        <i class="stroke-icon fa-sharp fa-solid fa-badge-percent"></i>
                                        <i class="fill-icon fa-sharp fa-solid fa-badge-percent"></i>

                                        <span>Discounts Code</span><div class="according-menu"><i class="fa fa-angle-right"></i></div>
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <div class="right-arrow" id="right-arrow"><i data-feather="arrow-right"></i></div>
                    </nav>
                </div>
            </div>
            <!-- Page Sidebar Ends-->
            <div class="page-body">
                <!-- Container-fluid starts-->
                @RenderBody()
                <!-- Container-fluid Ends-->
            </div>
            <!-- footer start-->
            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12 footer-copyright d-flex flex-wrap align-items-center justify-content-between">
                            <p class="mb-0" style="font-size:13px">Copyright © 2024 <strong>Al-Rakam Al-Awal Co.</strong> All rights reserved. </p>
                            <p class="mb-0" style="font-size:13px">
                                Powered by <a id="majesticAgency" style="font-size:13px" href="https://majesticagency.net/"><strong>Majestic Agency</strong></a>
                            </p>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
    </div>

    <!-- latest jquery-->
    <script src="~/admin/js/jquery.min.js"></script>
    <!-- Bootstrap js-->
    <script src="~/admin/js/bootstrap/bootstrap.bundle.min.js"></script>

    <!-- scrollbar js-->
    <script src="~/admin/js/scrollbar/simplebar.js"></script>
    <script src="~/admin/js/scrollbar/custom.js"></script>

    <!-- Sidebar jquery-->
    <script src="~/admin/js/config.js"></script>

    <!-- Plugins JS start-->
    <script src="~/admin/js/sidebar-menu.js"></script>
    <script src="~/admin/js/slick/slick.min.js"></script>
    <script src="~/admin/js/slick/slick.js"></script>
    <script src="~/admin/js/notify/bootstrap-notify.min.js"></script>
    <script src="~/admin/js/owlcarousel/owl.carousel.js"></script>
    <script src="~/admin/js/ecommerce.js"></script>
    <script src="~/admin/js/tagify.js"></script>
    <script src="~/admin/js/tagify.polyfills.min.js"></script>
    <script src="~/admin/js/intltelinput.min.js"></script>
    <script src="~/admin/js/custom-inputsearch.js"></script>
    <script src="~/admin/js/select3-custom.js"></script>
</body>

</html>
