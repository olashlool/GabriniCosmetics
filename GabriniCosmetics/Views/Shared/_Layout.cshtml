                                        <span class="cart-label">@Localizer["ShoppingCart"]</span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                        <script>
                            window.addEventListener("DOMContentLoaded", function () {
                                var n = document.getElementById("mobile-bottom-navigation");
                                document.body.appendChild(n);
                            });
                        </script>
