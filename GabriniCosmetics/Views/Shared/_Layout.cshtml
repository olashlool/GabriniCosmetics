<body>
      <div class="header-wrapper">
         <div class="header">
            <div class="announcement-wrapper announcement-swiper">
                <div class="swiper">
                    <div class="announcement-bar swiper-wrapper">
                        @{
                            var announcementBar = await AnnouncementBar.GetAllAsync();
                            var announcementBarFinal = culture.StartsWith("en") ? announcementBar.FirstOrDefault().AnnouncementEn : announcementBar.FirstOrDefault().AnnouncementAr;
                        }
                        <div class="swiper-slide">
                            <span class="announcement-item">@announcementBarFinal</span>
                        </div>
                    </div>
                </div>
                <button type="button" class="icon icon-close" id="announcement-close"></button>
            </div>

            <div class="header-upper">
               <div class="header-logo">
                   <button type="button" class="mobile-nav-toggle"> 
                       <span class="icon icon-nav"></span> 
                   </button>
                    <a href="/">
                        <img alt="Gabrini Cosmetics" src="~/images/gabrinicosmetics/gabrini-logo.png" />
                    </a>
               </div>
               <div class="header-right">
                  <div class="header-selectors-wrapper">
                     <div class="currency-selector">
                        <select id="customerCurrency" name="customerCurrency" onchange="setLocation(this.value)" aria-label="Currency selector">
                                <option selected value="#">@Localizer["JOD"]</option>
                        </select>
                     </div>
                     <div class="language-selector">
                            <select id="changeLanguage" name="customerlanguage" onchange="setLocation(this.value)" aria-label="Languages selector">
                                <option selected disabled>
                                    @{
                                        var cultureSelected = Thread.CurrentThread.CurrentUICulture.Name;
                                        var localizedText = cultureSelected == "en" ? Localizer["English"] : Localizer["Arabic"];
                                    }
                                    @localizedText 
                                </option>
                                <option value="/Home/Change?culture=en">@Localizer["English"]</option>
                                <option value="/Home/Change?culture=ar">@Localizer["Arabic"]</option>
                        </select>
                     </div>
                  </div>
                    <div class="header-links-wrapper">
                        <div class="header-links">
                            <ul>
                                <li>
                                    <a asp-controller="Wishlist" asp-action="index" class="ico-wishlist">
                                        <span class="wishlist-label">@Localizer["Wishlist"]</span>
                                        <span class="wishlist-qty">0</span>
                                    </a>
                                </li>
                                @if (!User.Identity.IsAuthenticated)
                                {
                                    <li class="user-dropdown dropdown">
                                        <a asp-controller="Account" asp-action="Login" class="ico-login">@Localizer["Login"]</a>
                                        <ul class="login-dropdown-menu ps-0">
                                            <li><a asp-controller="Account" asp-action="Register" class="ico-register">@Localizer["Register"]</a></li>
                                            <li><a asp-controller="Account" asp-action="Login" class="ico-login">@Localizer["Login"]</a></li>
                                        </ul>
                                    </li>
                                }
                                else
                                {
                                <li class="user-dropdown dropdown">
                                        <a asp-controller="Account" asp-action="Profile" class="ico-login">@Localizer["MyAccount"]</a>
                                    <ul class="login-dropdown-menu ps-0">
                                            <li><a asp-controller="Account" asp-action="Profile" class="ico-register">@Localizer["MyAccount"]</a></li>
                                            <li><a asp-controller="Account" asp-action="Logout" class="ico-login">@Localizer["Logout"]</a></li>
                                    </ul>
                                </li>
                                }
                                <ul>
                                    <li id="topcartlink">
                                        <a href="#" class="ico-cart">
                                            <span class="cart-label">@Localizer["ShoppingCart"]</span>
                                            <span id="cartCount" class="cart-qty">0 <span>items</span></span>
                                        </a>
                                    </li>
                                </ul>
                            </ul>
                        </div>
                        <div id="mobile-bottom-navigation">
                            <ul>
                                @if (!User.Identity.IsAuthenticated)
                                {
                                    <li class="mobile-user-dropdown dropdown">
                                        <a asp-controller="Account" asp-action="Login" class="ico-login">@Localizer["Login"]</a>
                                        <ul class="mobile-login-dropdown-menu ps-0">
                                            <li><a asp-controller="Account" asp-action="Register" class="ico-register">@Localizer["Register"]</a></li>
                                            <li><a asp-controller="Account" asp-action="Login" class="ico-login">@Localizer["Login"]</a></li>
                                        </ul>
                                    </li>
                                }
                                else
                                {
                                    <li class="mobile-user-dropdown dropdown">
                                        <a asp-controller="Account" asp-action="Profile" class="ico-login">@Localizer["MyAccount"]</a>
                                        <ul class="mobile-login-dropdown-menu ps-0">
                                            <li><a asp-controller="Account" asp-action="Register" class="ico-register">@Localizer["Register"]</a></li>
                                            <li><a asp-controller="Account" asp-action="Login" class="ico-login">@Localizer["Login"]</a></li>
                                        </ul>
                                    </li>
                                }

