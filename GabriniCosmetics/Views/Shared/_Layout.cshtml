                                    <li class="has-children">
                                        <a href="#"> <span class="category-name"> @Localizer["Categories"] </span> </a>
                                        <span class="sublist-toggle @iconClass"></span>
                                        <ul class="sublist">
                                            <li class="parent-back-link">
                                                <span class="parent-link-name"> <span class="@iconClassRes"></span> @Localizer["Categories"] </span>
                                                @inject GabriniCosmetics.Areas.Admin.Models.Interface.ICategory Category

                                                @foreach (var item in await Category.GetCategories())
                                                {
                                                    var nameCategory = culture.StartsWith("en") ? item.NameEn : item.NameAr;

                                                <li class="has-children">

                                                    <a asp-controller="Categories" asp-action="index" asp-route-id="@item.Id"> <span class="category-name"> @nameCategory </span> </a> <span class="sublist-toggle @iconClass"></span>
                                                    <ul class="sublist">
                                                        <li class="parent-back-link"><span class="parent-link-name"> <span class="@iconClassRes"></span> @nameCategory </span></li>
                                                    @foreach (var subCategory in item.SubCategories)
                                                    {
                                                            var nameSubCategory = culture.StartsWith("en") ? subCategory.NameEn : subCategory.NameAr;
                                                            <li> <span class="category-name"> @nameSubCategory </span></li>
                                                    }
                                                    </ul>
                                                </li>
                                                }
                                        </ul>
                                    </li>
                                    <li class="has-children">
                                        <a href="#"> <span class="category-name"> @Localizer["Products"] </span> </a> <span class="sublist-toggle @iconClass"></span>
                                        <ul class="sublist">
                                            <li class="parent-back-link"><span class="parent-link-name"> <span class="@iconClassRes"></span> @Localizer["Products"] </span>
                                            <li class="has-children">
                                                <a asp-controller="Products" asp-action="NewProducts"> <span class="category-name"> @Localizer["NewProduct"]</span> </a> <span class="sublist-toggle @iconClass"></span>
                                                <ul class="sublist">
                                                    <li class="parent-back-link"><span class="parent-link-name"> <span class="@iconClassRes"></span> @Localizer["NewProduct"] </span></li>
                                                    @inject GabriniCosmetics.Areas.Admin.Models.Interface.IProduct Product
                                                    @{
                                                        var products = await Product.GetProducts();
                                                        var newProducts = products.Where(p => p.Flags.Any(x => x.FlagType == "New")).ToList();
                                                    }
                                                    @foreach (var item in newProducts)
                                                    {
                                                        var name = culture.StartsWith("en") ? item.Product.NameEn : item.Product.NameAr;
                                                        <li> <span class="category-name"> @name </span></li>
                                                    }
                                                </ul>
                                            </li>
                                            <li class="has-children">
                                                <a asp-controller="Products" asp-action="SaleProducts"> <span class="category-name"> @Localizer["BestSaleProduct"]</span> </a> <span class="sublist-toggle @iconClass"></span>
                                                <ul class="sublist">
                                                    <li class="parent-back-link"><span class="parent-link-name"> <span class="@iconClassRes"></span> @Localizer["BestSaleProduct"] </span></li>
                                                    @{
                                                        var saleProducts = products.Where(p => p.Flags.Any(x => x.FlagType == "Sale")).ToList();
                                                    }
                                                    @foreach (var item in saleProducts)
                                                    {
                                                        var name = culture.StartsWith("en") ? item.Product.NameEn : item.Product.NameAr;
                                                        <li> <span class="category-name"> @name </span></li>
                                                    }
                                                </ul>
                                            </li>
                                            <li class="has-children">
                                                <a asp-controller="Products" asp-action="FeatureProducts"> <span class="category-name"> @Localizer["FeatureProducts"]</span> </a> <span class="sublist-toggle @iconClass"></span>
                                                <ul class="sublist">
                                                    <li class="parent-back-link"><span class="parent-link-name"> <span class="@iconClassRes"></span> @Localizer["FeatureProducts"] </span></li>
                                                    @{
                                                        var featureProducts = products.Where(p => p.Flags.Any(x => x.FlagType == "Feature")).ToList();
                                                    }
                                                    @foreach (var item in featureProducts)
                                                    {
                                                        var name = culture.StartsWith("en") ? item.Product.NameEn : item.Product.NameAr;
                                                        <li> <span class="category-name"> @name </span></li>
                                                    }
                                                </ul>
                                            </li>
                                            <li>
                                                <a asp-controller="Products" asp-action="index"> <span class="category-name"> @Localizer["AllProducts"]</span> </a> <span class="sublist-toggle @iconClass"></span>
                                            </li>
                                        </ul>
                                    </li>
                                    <li><a asp-controller="Contactus" asp-action="index"> <span class="category-name"> @Localizer["Contactus"]</span> </a>
                                </ul>
                            </nav>
                        </div>
                    </div>
                    <div class="megamenu-right"></div>
                </div>
            </div>
        </div>
      </div>
